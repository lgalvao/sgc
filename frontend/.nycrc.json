{
  "reporter": [
    "html",
    "text"
  ],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "all": true,
  "exclude": [
    "node_modules/**",
    "tests/**",
    "spec/**",
    "playwright.config.ts",
    "vite.config.js",
    "vitest.setup.js",
    "vitest.setup.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/main.ts",
    "src/router.ts"
  ]
}